{
  "version": 2,
  "name": "nextjs-contentful",
  "alias": "nextjs-contentful.now.sh",
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "build": {
    "env": {
      "SPACE_ID": "@space_id",
      "ACCESS_TOKEN": "@access_token"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/",
      "headers": {
        "cache-control": "s-maxage=300 maxage=0,stale-white-revalidate"
      }
    }
  ]
}
